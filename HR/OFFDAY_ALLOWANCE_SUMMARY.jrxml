	<field name="ACTUAL_TIME" class="java.math.BigDecimal"/>
	<field name="IN_TIME" class="java.lang.String"/>
	<field name="OUT_TIME" class="java.lang.String"/>
	<field name="TOTAL_PAY_AMT" class="java.math.BigDecimal"/>
	<field name="OFFDAY_THIS_MONTH" class="java.math.BigDecimal"/>
	<field name="ACCOUNT_NO" class="java.lang.String"/>
	<field name="NET_PAYABLE_BANK" class="java.math.BigDecimal"/>
	<field name="NET_PAYABLE_CASH" class="java.math.BigDecimal"/>
