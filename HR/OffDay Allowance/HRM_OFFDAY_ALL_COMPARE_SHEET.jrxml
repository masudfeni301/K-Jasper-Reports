	<parameter name="P_MNYR" class="java.lang.String"/>
	<parameter name="P_COMPANY_CODE" class="java.lang.String"/>
	<parameter name="P_COMPANY_BRANCH_CODE" class="java.lang.String"/>
	<parameter name="P_DESIG_GROUP" class="java.lang.String"/>
	<parameter name="P_DEPT_CODE" class="java.lang.String"/>
	<parameter name="P_EMP" class="java.lang.String"/>
	<parameter name="P_DEPT_GROUP" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT SUM(TOT_STAFF) TOT_STAFF, SUM(OFFDAY_THIS_MONTH) OFFDAY_THIS_MONTH,  SUM(TOT_PAYABLE_DAYS) TOT_PAYABLE_DAYS, SUM(TOT_PAY_AMT) TOT_PAY_AMT, '  '||SECTION_NAME SECTION_NAME, 
SUM(TOT_STAFF_P) TOT_STAFF_P, SUM(OFFDAY_THIS_MONTH_P) OFFDAY_THIS_MONTH_P, SUM(TOT_PAYABLE_DAYS_P) TOT_PAYABLE_DAYS_P, SUM(TOT_PAY_AMT_P) TOT_PAY_AMT_P,
TO_CHAR(ADD_MONTHS(TO_DATE($P{P_MNYR}, 'MM/YYYY'),-1), 'fmMonth-YYYY') PREV_MONTH,  TO_CHAR(TO_DATE($P{P_MNYR}, 'MM/YYYY'), 'fmMonth-YYYY') THIS_MONTH
FROM (SELECT COUNT(DISTINCT EMP_ID) TOT_STAFF,

(SELECT COUNT(HOLIDAY_DATE) 
FROM ( SELECT HOLIDAY_DATE HOLIDAY_DATE
FROM HOLIDAY_WEEK
WHERE 
COMPANY_CODE        = $P{P_COMPANY_CODE} AND
COMPANY_BRANCH_CODE = $P{P_COMPANY_BRANCH_CODE} AND
TO_CHAR(HOLIDAY_DATE,'MM/YYYY') = $P{P_MNYR}
UNION
SELECT DATE_TO HOLIDAY_DATE
FROM HOLIDAY
WHERE 
COMPANY_CODE        = $P{P_COMPANY_CODE} AND
COMPANY_BRANCH_CODE = $P{P_COMPANY_BRANCH_CODE} AND
TO_CHAR(FROM_DATE,'MM/YYYY')   = $P{P_MNYR}
)) as OFFDAY_THIS_MONTH, 

COUNT(ATTEND_DATE) as TOT_PAYABLE_DAYS,         
SUM(TOTAL_PAY_AMT) AS TOT_PAY_AMT, 
DEPARTMENT, SECTION_NAME, DEPT_GROUP, DEPT_CODE, DEPT_GROUP_SL,
NULL TOT_STAFF_P, NULL OFFDAY_THIS_MONTH_P, NULL TOT_PAYABLE_DAYS_P, NULL TOT_PAY_AMT_P
FROM (SELECT 
NAME, DESIGNATION, DEPARTMENT, SECTION_NAME, ATTEND_DATE , DD, EMP_ID,DUTY,
BASIC_SALARY, JOIN_DT, BANK_SAL_FLAG, EMP_AC_NO, EMP_AC_DATE, MOBILE_BANK_SAL_FLAG, MOBILE_AC_NO, MOBILE_AC_DATE, DEPT_GROUP, DEPT_CODE, 
DESIG_CODE, SALARY_TYPE, DESIG_GROUP, DEPT_GROUP_SL, OFFDAY_ALL, OFFDAY_LUNCH_DIN_ALL, 
IN_TIME, OUT_TIME, NVL(OFFDAY_ALL, 0)+NVL(OFFDAY_LUNCH_DIN_ALL, 0) as TOTAL_PAY_AMT
FROM (SELECT
NAME, DESIGNATION, DEPARTMENT, SECTION_NAME, ATTEND_DATE , DD, EMP_ID,DUTY,
BASIC_SALARY, JOIN_DT, EMP_AC_NO, EMP_AC_DATE, MOBILE_AC_NO, MOBILE_AC_DATE, DEPT_GROUP, DEPT_CODE, 
DESIG_CODE, SALARY_TYPE, DESIG_GROUP, DEPT_GROUP_SL, BANK_SAL_FLAG, MOBILE_BANK_SAL_FLAG,
(CASE 
WHEN PRE_ADJ_OFFDAY_ALL > 0 THEN 0
    ELSE PRE_OFFDAY_ALL
END) as OFFDAY_ALL,

(CASE 
WHEN PRE_ADJ_OFFDAY_LUNCH_ALL > 0 THEN 0
ELSE 
 CASE WHEN OUT_TIME1 IS NOT NULL AND OUT_TIME1>= TO_CHAR(TO_DATE('1400','HH24:MI'),'HH24:MI')  AND OUT_TIME1< TO_CHAR(TO_DATE('23:00','HH24:MI'),'HH24:MI') OR
 DUTY > 4 THEN 
  PRE_OFFDAY_LUNCH_ALL
 ELSE
    CASE WHEN ADJ_OFFDAY_LUNCH_ALL > 0 THEN PRE_OFFDAY_LUNCH_ALL
    ELSE 0
    END
 END    
END) as OFFDAY_LUNCH_DIN_ALL,
 
(CASE
WHEN PRE_IN_TIME > 0 THEN ORIGIN_IN_TIME
ELSE
IN_TIME1
END) as IN_TIME,

(CASE
WHEN PRE_IN_TIME > 0 THEN ORIGIN_OUT_TIME
ELSE
OUT_TIME1
END) as OUT_TIME
FROM (SELECT 
(SELECT ' '||O.DESIG_DESC FROM DESIGNATION O WHERE O.DEPT_GROUP = X.DEPT_GROUP AND O.DEPT_CODE  = X.DEPT_CODE AND O.DESIG_CODE = X.DESIG_CODE) as DESIGNATION,
(SELECT ' '||O.DEPT_GROUP_NAME FROM DEPT_GROUP O WHERE O.DEPT_GROUP_CODE = X.DEPT_GROUP) as DEPARTMENT,
(SELECT ' '||O.DEPT_DESC FROM DEPT O WHERE O.DEPT_GROUP = X.DEPT_GROUP AND O.DEPT_CODE  = X.DEPT_CODE) as SECTION_NAME,
ATTEND_DATE , DD, EMP_ID, NAME, IN_TIME as IN_TIME_DATE, OUT_TIME as OUT_TIME_DATE, IN_TIME1, OUT_TIME1, ABS(TO_NUMBER(TO_CHAR(IN_TIME,'HH24'))-  TO_NUMBER(TO_CHAR(OUT_TIME,'HH24'))) DUTY,
